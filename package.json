  },
  "lint-staged": {
    "*.{js,css,json,md,ts,tsx}": [
      "prettier --write"
    ]
  },
  "prettier": "@dmsi/prettier-config",
  "devDependencies": {
    "@dmsi/prettier-config": "^1.3.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dot-prop": "^5.2.0",
    "got": "^11.8.5",
    "polka": "^0.5.2"
  }
}
